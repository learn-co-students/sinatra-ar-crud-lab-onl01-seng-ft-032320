
<h1>Edit your article</h1>

<form action="/articles/<%= @article.id %>" method="post">
    <input id="hidden" type="hidden" name="_method" value="PATCH">
    <h2>Title:</h2>
    <p><input type="text" id="title" name="title" value=<%=@article.title%>></p>
    <h2>Content:</h2>
   <p><input type="text" id="content" name="content" value=<%=@article.content%>></p>
    <input type="submit" value="submit">
</form>
